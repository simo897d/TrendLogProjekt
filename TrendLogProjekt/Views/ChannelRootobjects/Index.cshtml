@model IEnumerable<TrendLogProjekt.Models.ChannelRootobject>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
           @foreach (var t in Model) {

            <th>@t.channel)</th>
           }
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ChannelId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ChannelId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ChannelId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
